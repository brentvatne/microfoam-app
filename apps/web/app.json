{
  "expo": {
    "name": "microfoam-web",
    "slug": "microfoam-web",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "web"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "678c395c-6391-4294-8ec0-1df5a81c8a59"
      }
    },
    "ios": {
      "bundleIdentifier": "com.brents.microfoamweb"
    }
  }
}
