{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "resourceClass": "large",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "channel": "base"
    },
    "update-base": {
      "config": "update.yml",
      "withoutCredentials": "true",
      "env": {
        "EAS_NO_VCS": "1"
      },
      "channel": "update-base"
    },
    "update-production": {
      "extends": "update-base",
      "channel": "production"
    },
    "update-preview": {
      "extends": "update-base",
      "channel": "preview"
    },
    "test": {
      "config": "run-tests.yml",
      "withoutCredentials": "true",
      "extends": "base",
      "channel": "test"
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true,
      "channel": "development"
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "PREVIEW": "1"
      }
    },
    "development-simulator": {
      "extends": "base",
      "developmentClient": true,
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "preview-simulator": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "PREVIEW": "1"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "RELEASE": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1645902499"
      }
    }
  }
}
