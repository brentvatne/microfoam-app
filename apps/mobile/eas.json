{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "resourceClass": "large",
      "environment": "development",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    },
    "test": {
      "config": "run-tests.yml",
      "withoutCredentials": true,
      "extends": "base"
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    },
    "development-simulator": {
      "extends": "base",
      "developmentClient": true,
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "environment": "preview",
      "distribution": "internal"
    },
    "preview-simulator": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "environment": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "channel": "production",
      "environment": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1645902499"
      }
    }
  }
}
