{
  "name": "microfoam-mobile",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "test": "jest",
    "android": "expo run:android --all-arch",
    "update:production": "yarn fingerprint:validate && eas update --channel production",
    "update:preview": "yarn fingerprint:validate && eas update --channel preview",
    "prebuild:android": "./scripts/prebuild-android.sh",
    "prebuild:ios": "./scripts/prebuild-ios.sh",
    "fingerprint:validate": "ts-node scripts/validate-fingerprint.ts",
    "fingerprint:set": "ts-node scripts/set-fingerprint.ts",
    "prettier:fix": "prettier app components constants storage index.js -w",
    "fix-deps": "npx expo install --check",
    "spotlessCore": "cd ../../expo/apps/bare-expo/android && ./gradlew :expo-modules-core:spotlessApply",
    "spotlessCoreTests": "cd ../../expo/apps/bare-expo/android && ./gradlew :expo-modules-test-core:spotlessApply",
    "spotlessNotif": "cd ../../expo/apps/bare-expo/android && ./gradlew :expo-notifications:spotlessApply",
    "spotlessAll": "cd ../../expo/apps/bare-expo/android && ./gradlew spotlessApply"
  },
  "dependencies": {
    "@expo/fingerprint": "^0.6.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-segmented-control/segmented-control": "2.5.2",
    "@shopify/flash-list": "1.6.4",
    "@supabase/supabase-js": "^2.0.0-rc.10",
    "assert": "^2.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "burnt": "^0.12.2",
    "color": "^4.2.3",
    "date-fns": "^2.29.3",
    "expo": "*",
    "expo-application": "~5.9.1",
    "expo-atlas": "^0.1.1",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-drag-drop-content-view": "^0.2.0",
    "expo-file-system": "~17.0.1",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.12.14",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.0.7",
    "expo-insights": "~0.7.0",
    "expo-linking": "~6.3.1",
    "expo-media-library": "~16.0.4",
    "expo-modules-core": "*",
    "expo-modules-test-core": "*",
    "expo-notifications": "*",
    "expo-quick-actions": "^0.2.3",
    "expo-router": "~3.5.23",
    "expo-screen-orientation": "~7.0.5",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-task-manager": "~11.8.2",
    "expo-updates": "*",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "lodash": "^4.17.21",
    "moti": "^0.20.0",
    "pretty-bytes": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.75.0",
    "react-native-blurhash": "^1.1.10",
    "react-native-date-picker": "^4.2.5",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.15.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.34.0",
    "react-native-url-polyfill": "^1.3.0",
    "string-to-color": "^2.2.2",
    "tinybase": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@expo/json-file": "^8.3.0",
    "@types/color": "^3.0.3",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.79",
    "babel-plugin-module-resolver": "^5.0.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "expo",
        "expo-modules-core",
        "expo-notifications",
        "react-native@~0.74.0",
        "react-native-reanimated@~3.10.0",
        "react-native-gesture-handler@~2.16.1",
        "react-native-screens@~3.31.1"
      ]
    },
    "autolinking": {
      "android": {
        "exclude": [
          "expo-quick-actions"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
