{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      // "node": "14.2.0",
      "ios": {
        "resourceClass": "large"
      },
      "env": {
        "SENTRY_ORG": "brents",
        "SENTRY_PROJECT": "microfoam",
        "EAS_BUILD_ENABLE_COCOAPODS_CACHE": "1"
        // "NVM_NODEJS_ORG_MIRROR": "https://nodejs.raccoon-tw.dev/release"
      }
    },
    "development": {
      "extends": "base",
      // "channel": "development",
      "distribution": "internal",
      "developmentClient": true
    },
    "preview": {
      "extends": "base",
      // "channel": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "PREVIEW": "1"
      }
    },
    "production": {
      "extends": "base",
      // "channel": "production",
      "autoIncrement": true,
      "env": {
        "RELEASE": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1645902499"
      }
    }
  }
}
