{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "large"
      },
      "env": {
        "SENTRY_ORG": "brents",
        "SENTRY_PROJECT": "microfoam",
        "EXPO_PUBLIC_VEXO_API_KEY": "1003c65c-f56a-4769-9ed9-8da11754b0af",
        "EAS_BUILD_PROJECT_ID": "f19296df-44bd-482a-90bb-2af254c6ac42"
      }
    },
    "development": {
      "extends": "base",
      // "channel": "development",
      "distribution": "internal",
      "developmentClient": true
    },
    "preview": {
      "extends": "base",
      // "channel": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "PREVIEW": "1"
      }
    },
    "production": {
      "extends": "base",
      // "channel": "production",
      "autoIncrement": true,
      "env": {
        "RELEASE": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1645902499"
      }
    }
  }
}
